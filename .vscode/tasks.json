{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build Preprocessing",
      "type": "shell",
      "command": "./gradlew :preprocessing:build",
      "group": "build",
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared",
        "showReuseMessage": false,
        "clear": true
      }
    },
    {
      "label": "Run Preprocessing",
      "type": "shell",
      "command": "./preprocessing/build/exe/main/debug/preprocessing",
      "group": "build",
      "dependsOn": [
        "Build Preprocessing"
      ],
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared",
        "showReuseMessage": false,
        "clear": true
      }
    }
  ]
}
